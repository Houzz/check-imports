  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/finom/check-imports"
  },
  "dependencies": {
    "@babel/parser": "^7.5.5",
    "@babel/traverse": "^7.5.5",
    "chalk": "^2.4.2",
    "commander": "^3.0.0",
    "find-up": "^4.1.0",
    "glob-promise": "^3.4.0",
    "npm-api": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "expect.js": "^0.3.1"
  }
